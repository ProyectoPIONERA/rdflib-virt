@prefix rml: <http://w3id.org/rml/> .
@prefix ub: <http://swat.cse.lehigh.edu/onto/univ-bench.owl#> .

<#TM1> a rml:TriplesMap ;
    rml:logicalSource [ rml:tablename "university" ] ;
    rml:predicateObjectMap [ rml:graphMap [ rml:constant rml:defaultGraph ;
                    rml:termType rml:IRI ] ;
            rml:objectMap [ rml:constant ub:university ;
                    rml:termType rml:IRI ] ;
            rml:predicateMap [ rml:constant ub:type ;
                    rml:termType rml:IRI ] ] ;
    rml:subjectMap [ rml:template "http://www.university{nr}.edu" ;
            rml:termType rml:IRI ] .

<#TM11> a rml:TriplesMap ;
    rml:logicalSource [ rml:query """
        SELECT researchGroup.nr AS researchgroupnr, researchGroup.subOrganizationOf AS departmentnr, department.subOrganizationOf AS universitynr
        FROM researchGroup INNER JOIN department ON researchGroup.subOrganizationOf=department.nr """ ;
            rml:referenceFormulation rml:SQL2008 ] ;
    rml:predicateObjectMap [ rml:graphMap [ rml:constant rml:defaultGraph ;
                    rml:termType rml:IRI ] ;
            rml:objectMap [ rml:constant ub:researchgroup ;
                    rml:termType rml:IRI ] ;
            rml:predicateMap [ rml:constant ub:type ;
                    rml:termType rml:IRI ] ] ;
    rml:subjectMap [ rml:template "http://www.department{departmentnr}.university{universitynr}.edu/researchGroup{researchgroupnr}.edu" ;
            rml:termType rml:IRI ] .

